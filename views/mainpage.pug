html
  head
    title Future Imperfect by HTML5 UP
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    //if lte IE 8
      script(src='assets/js/ie/html5shiv.js')
    link(rel='stylesheet', href='assets/css/main.css')
    //if lte IE 9
      link(rel='stylesheet', href='assets/css/ie9.css')
    //if lte IE 8
      link(rel='stylesheet', href='assets/css/ie8.css')
  body
    // Wrapper
    #wrapper
      // Header
      header#header
        h1
          a(href='/') Go on Trip
        nav.links
          ul
            li
              a(href='/Introduction') Introduction
            li
              a(href='/Community') Community
            li
              a(href='/Information') information
            li
              a(href='/QandA') Q&A
        nav.main
          if nickName
            ul
              li
                a Welcome 
                =nickName
              li
                a.button.small.fit(href='/logout') Logout
          else
            ul
              li
                a.button.small.fit(href='/login') Log In
              li
                a.button.small.fit(href='/register') Register
      // Main
      #main
        // Post
        each timeline in timelines
          article.post
            header
              .title
                h2
                  a(href='#') Best
                p=timeline.timelinetitle
              .meta
                h3=timeline.timelinetimeat
                a.author(href='#')
                  span.name=timeline.nickName
                  img(src='images/uploads/'+timeline.userimg, alt='')
            a.image.featured(href='#')
              img(src='images/uploads/'+timeline.timelineimg, alt='')
            p=timeline.timelinecontent
            footer
              ul.actions
                li
                  a.button.big(href='#') Continue Reading
              ul.stats
                li
                  a(href='#') General
                li
                  a.icon.fa-heart(href='/timelinelikey/'+timeline.id)=timeline.likey
                
        
        // Pagination
        ul.actions.pagination
          li
            a.disabled.button.big.previous(href='') Previous Page
          li
            a.button.big.next(href='#') Next Page
      // Sidebar
      section#sidebar
        // Intro
        section#intro
          a.logo(href='/')
            img(src='images/logo.png', alt='')
          header
            h2 Go on Trip
            p GOONT(Go on trip) provide community with tourist information 
        // Mini Posts
        section
          .mini-posts
            p  MINI-POST
            // Mini Post
            article.mini-post
              header
                h3
                  a(href='#') 순천만 별빛축제
                time.published(datetime='2015-10-20') november 8 2017 ~ december 7 2018
                
              a.image(href='#')
                img(src='images/pic04.png', alt='')
            // Mini Post
            article.mini-post
              header
                h3
                  a(href='#') 보령 머드축제
                time.published(datetime='2015-10-19') October 19, 2015
              a.image(href='#')
                img(src='images/mud.png', alt='')
            // Mini Post
            article.mini-post
              header
                h3
                  a(href='#') 태백 눈꽃축제
                time.published(datetime='2015-10-18') October 18, 2015
              a.image(href='#')
                img(src='images/snowflower.png', alt='')
            // Mini Post
            article.mini-post
              header
                h3
                  a(href='#') 지산 락페스티벌
                time.published(datetime='2015-10-17') October 17, 2015
              a.image(href='#')
                img(src='images/jisan.png', alt='')
        
        // About
        section.blurb
          h2 About
          p
            | Mobile: 010.2537.1907 
            br
            | 									E-Mail: ghdjk2216@gmail.com 
            br
            | 								Post: Seoul, Mapo, Sang-su 86-12
          ul.actions
            li
              a.button(href='#') Learn More
        // Footer
        section#footer
          ul.icons
            li
              a.fa-twitter(href='#')
                span.label Twitter
            li
              a.fa-facebook(href='#')
                span.label Facebook
            li
              a.fa-instagram(href='#')
                span.label Instagram
            li
              a.fa-rss(href='#')
                span.label RSS
            li
              a.fa-envelope(href='#')
                span.label Email
          p.copyright
            | © Untitled. Design: 
            a(href='http://html5up.net') HTML5 UP
            | . Images: 
            a(href='http://unsplash.com') Unsplash
            | .
    // Scripts
    script(src='assets/js/jquery.min.js')
    script(src='assets/js/skel.min.js')
    script(src='assets/js/util.js')
    //if lte IE 8
      script(src='assets/js/ie/respond.min.js')
    script(src='assets/js/main.js')
